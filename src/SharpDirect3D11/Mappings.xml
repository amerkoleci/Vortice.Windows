  <depends>SharpDXGI</depends>

  <sdk name="WindowsSdk" version="10.0.17763.0" />
  <sdk name="StdLib" />

  <include-prolog>
    #define D3D11_NO_HELPERS
  </include-prolog>

  <include file="d3d11.h"
             namespace="SharpDirect3D11"
             attach="true"/>

  <include file="d3d11_1.h"
             namespace="SharpDirect3D11"
             attach="true"/>

  <include file="d3d11_2.h"
             namespace="SharpDirect3D11"
             attach="true"/>

